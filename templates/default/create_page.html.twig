{% extends 'base.html.twig' %} {# recuperer la base#}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/style_contact.css') }}">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

{% endblock %}

{% block body %}
    <div class="container contact-form">
    <div class="contact-image">
        <img src="https://image.ibb.co/kUagtU/rocket_contact.png" alt="rocket_contact"/>
    </div>

    <div class="row">
    <div class="col-md-12 bg-primary">
    </div>
    <div class="col-md-12">
    <div class="form">
    {{ form_start(form_create) }} {# demarer #}
    {{ form_widget(form_create) }} {# champs #}
    <input type="submit" name="submit" value="enregistrer">
    {{ form_end(form_create) }}

    <a href="#" onclick="history.back();">Retour</a> {# boutton retour #}

{% endblock %}

